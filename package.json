{
  "name": "whats-the-weather",
  "version": "1.0.0",
  "scripts": {
  "start": "node backend/index.js",
  "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
  "dev-backend": "cd backend && nodemon index.js",
    "dev-frontend": "cd vue-project && npm run dev"
  },
  "postinstall": "cd vue-project && npm install && npm run build",
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}